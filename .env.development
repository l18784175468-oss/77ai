# 开发环境配置
NODE_ENV=development
PORT=5000

# 数据库配置
DATABASE_URL=mongodb://localhost:27017/ai_web_framework_dev

# JWT配置
JWT_SECRET=dev-jwt-secret-key-change-in-production
JWT_EXPIRES_IN=7d

# AI服务配置
OPENAI_API_KEY=your-openai-api-key-for-development
ANTHROPIC_API_KEY=your-anthropic-api-key-for-development
GOOGLE_AI_API_KEY=your-google-ai-api-key-for-development

# CORS配置
CORS_ORIGIN=http://localhost:3000

# 文件上传配置
MAX_FILE_SIZE=10485760
UPLOAD_PATH=./uploads

# 日志配置
LOG_LEVEL=debug
LOG_FILE=./logs/app.log

# 前端配置
VITE_API_BASE_URL=http://localhost:5000/api
VITE_APP_TITLE=AI Web Framework (Dev)
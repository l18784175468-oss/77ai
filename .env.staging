# 预发布环境配置
NODE_ENV=staging
PORT=5000

# 数据库配置
DATABASE_URL=mongodb://admin:password@mongodb:27017/ai_web_framework_staging?authSource=admin

# Redis配置（可选）
# REDIS_URL=redis://redis:6379

# JWT配置
JWT_SECRET=staging-jwt-secret-key-change-in-production
JWT_EXPIRES_IN=7d

# AI服务配置
OPENAI_API_KEY=your-openai-api-key-for-staging
ANTHROPIC_API_KEY=your-anthropic-api-key-for-staging
GOOGLE_AI_API_KEY=your-google-ai-api-key-for-staging

# CORS配置
CORS_ORIGIN=https://staging.yourdomain.com

# 文件上传配置
MAX_FILE_SIZE=10485760
UPLOAD_PATH=./uploads

# 日志配置
LOG_LEVEL=info
LOG_FILE=./logs/app.log

# 前端配置
VITE_API_BASE_URL=https://api-staging.yourdomain.com
VITE_APP_TITLE=AI Web Framework (Staging)